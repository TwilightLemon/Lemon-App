<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  DarkMode  -->
    <!--  主窗口背景色 对于Mica材质不适用  -->
    <SolidColorBrush x:Key="WindowBackgroundColor" Color="#9F000000" />
    <!--  弹窗背景色 全为Acrylic材质  -->
    <SolidColorBrush x:Key="PopupWndBgColor" Color="#9F000000" />
    <!--  标签页背景色  -->
    <SolidColorBrush x:Key="TabMaskColor" Color="#7C000000" />
    <!--  前景色  -->
    <SolidColorBrush x:Key="ForeColor" Color="#FCFCFC" />
    <!--  提示前景色  -->
    <SolidColorBrush x:Key="HintForeColor" Color="#9C9C9C" />
    <!--  蒙罩背景色  -->
    <SolidColorBrush x:Key="MaskColor" Color="#0cffffff" />
    <!--  焦点蒙罩背景色  -->
    <SolidColorBrush x:Key="FocusMaskColor" Color="#33FFFFFF" />
    <!--  主窗口的内容容器背景色  -->
    <SolidColorBrush x:Key="ContentBgColor" Color="#6F000000" />
    <SolidColorBrush x:Key="BorderColor" Color="#3C000000" />
    <!--  纯色背景  -->
    <SolidColorBrush x:Key="BackgroundColor" Color="#FF1C1C1C" />
    <SolidColorBrush x:Key="ButtonPureColor" Color="#454545" />
    <!--  LyricPage  -->
    <SolidColorBrush x:Key="InActiveLrcForeground" Color="#32FFFFFF" />
</ResourceDictionary>