<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--DarkMode-->
    <!--高亮主题色-->
    <SolidColorBrush x:Key="HighlightThemeColor"
                     Color="#69F" />
    <!--窗口背景色-->
    <SolidColorBrush x:Key="WindowBackgroundColor"
                     Color="#CC000000" />
    <!--标签页背景色-->
    <SolidColorBrush x:Key="TabMaskColor"
                     Color="#4C000000" />
    <!--前景色-->
    <SolidColorBrush x:Key="ForeColor"
                     Color="#FEFEFE" />
    <!--蒙罩背景色-->
    <SolidColorBrush x:Key="MaskColor"
                     Color="#19ffffff" />
    <!--焦点蒙罩背景色-->
    <SolidColorBrush x:Key="FocusMaskColor"
                     Color="#33FFFFFF" />
    <!--纯色背景-->
    <SolidColorBrush x:Key="BackgroundColor"
                     Color="#000" />
</ResourceDictionary>