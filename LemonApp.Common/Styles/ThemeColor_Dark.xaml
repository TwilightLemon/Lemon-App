<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--DarkMode-->
    <!--窗口背景色-->
    <SolidColorBrush x:Key="WindowBackgroundColor"
                     Color="#CC000000" />
    <!--标签页背景色-->
    <SolidColorBrush x:Key="TabMaskColor"
                     Color="#6C000000" />
    <!--前景色-->
    <SolidColorBrush x:Key="ForeColor"
                     Color="#FCFCFC" />
    <!--蒙罩背景色-->
    <SolidColorBrush x:Key="MaskColor"
                     Color="#0cffffff" />
    <!--焦点蒙罩背景色-->
    <SolidColorBrush x:Key="FocusMaskColor"
                     Color="#33FFFFFF" />
    <SolidColorBrush x:Key="BorderColor"
                     Color="#3C000000" />
    <!--纯色背景-->
    <SolidColorBrush x:Key="BackgroundColor"
                     Color="#FF1C1C1C" />
</ResourceDictionary>